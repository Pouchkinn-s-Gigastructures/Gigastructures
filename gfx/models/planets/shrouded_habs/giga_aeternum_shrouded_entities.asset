		start_event = { trigger_once = yes sound = { soundeffect = "shroud_creature_idle_high" } }
		event = { time = 1.2 trigger_once = yes sound = { soundeffect = "shroud_creature_thunder" } }

	}

	game_data = { 
		bloom_factor = 1.0 
		uv_animation_speed 		= 3 #faster UV animation than normal shroud creatures to help with the feel that its chaotic swirling
		uv_animation_direction 	= { 0.0 -0.04 }
	}
}
