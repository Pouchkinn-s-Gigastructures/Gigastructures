	locator = { name = "cannon_halfsouth_center" position = { 0 1.3 4.2 } rotation = { 0 93 0 } }
	locator = { name = "cannon_halfnorth_back" position = { 0 -1.3 -4.2 } rotation = { -180 93 180 } }
	locator = { name = "cannon_halfsouth_back" position = { 0 1.3 -4.2 } rotation = { 0 87 180 } }

	# Rings
