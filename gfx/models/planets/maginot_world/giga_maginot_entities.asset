	attach = { top_station = "pc_giga_maginot_world_orbital_top_entity" }
	attach = { bottom_station = "pc_giga_maginot_world_orbital_top_entity" }
	# Surface greebles
	attach = { greebles = "pc_giga_maginot_world_greeble_entity" }
	# Equatorial planetary cannons
	attach = { cannon_equator_right = "pc_giga_maginot_world_cannon_double_entity" }
	attach = { cannon_halfnorth_right = "pc_giga_maginot_world_kinetic_s_entity" }
	attach = { cannon_halfsouth_right = "pc_giga_maginot_world_kinetic_s_entity" }
	# Equatorial second group
	attach = { cannon_equator_left = "pc_giga_maginot_world_cannon_double_entity" }
	attach = { cannon_halfnorth_left = "pc_giga_maginot_world_kinetic_s_entity" }
	attach = { cannon_halfsouth_left = "pc_giga_maginot_world_kinetic_s_entity" }
	# Orthogonal cannons
	attach = { cannon_halfnorth_center = "pc_giga_maginot_world_laser_l_entity" }
	attach = { cannon_halfsouth_center = "pc_giga_maginot_world_laser_l_entity" }
	attach = { cannon_halfnorth_back = "pc_giga_maginot_world_laser_l_entity" }
	attach = { cannon_halfsouth_back = "pc_giga_maginot_world_laser_l_entity" }

	meshsettings = {
	name = "planet_geosphereShape"
	shader = "PdxMeshPlanetEmissive"
		texture_diffuse = "giga_maginot_world_diffuse.dds"
		texture_normal = "giga_maginot_world_normal.dds"
		texture_specular = "giga_maginot_world_specular.dds"
	}

	default_state = "idle"
	playback_rate = 1.0 # Rotation is reversed
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }	}
}

