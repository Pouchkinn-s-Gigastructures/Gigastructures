entity = {
	name = "pc_giga_maginot_world_01_entity"
	pdxmesh = "planet_clouded_mesh"

	# surface ships
	attach = { planet = "pc_giga_maginot_world_surface_ships_entity" }
	locator = { name = "shipx2" rotation = { 45 0 180 } }
	attach = { shipx2 = "pc_giga_maginot_world_surface_ships_x2_entity" }
	locator = { name = "shipx3" rotation = { 90 90 90 } } # rotation seems irrelevant...?
	attach = { shipx3 = "pc_giga_maginot_world_surface_ships_x3_entity" }
	# locator shenanigans
	attach = { planet = "pc_giga_maginot_world_dummy_entity" }

	# surface texture
	meshsettings = {
		name = "planet_geosphereShape"
		shader = "PdxMeshPlanetEmissive"
		texture_diffuse = "giga_maginot_world_diffuse.dds"
		texture_normal = "giga_maginot_world_normal.dds"
		texture_specular = "giga_maginot_world_specular.dds"
	}
	# "cloud" texture (actually flying forts)
	meshsettings = {
		name = "clouds_geosphereShape"
		shader = "PdxMeshClouds"
		texture_diffuse = "clouds_maginot_01_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
	}	
	# cloud shadow texture
	meshsettings = {
		name = "clouds_shadow_geosphereShape"
		texture_diffuse = "clouds_maginot_shadow_01_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
	}

	default_state = "idle"
	playback_rate = 1.0 # Rotation is reversed
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
	start_event = { trigger_once = yes sound = { soundeffect = "amb_pc_maginot_hum_layer_01_sfx" } }
	start_event = { trigger_once = yes sound = { soundeffect = "amb_pc_maginot_hum_layer_02_sfx" } }
	start_event = { trigger_once = yes sound = { soundeffect = "amb_pc_maginot_telemetry_01_sfx" } }
	start_event = { trigger_once = yes sound = { soundeffect = "amb_city_planet" } }
	start_event = { trigger_once = yes sound = { soundeffect = "amb_city_planet_flyby" } }
	}
}

# locator shenanigans, makes stuff rotate with planet
# ACOT does this for the void sphere and it works
entity = {
	name = "pc_giga_maginot_world_dummy_entity"
	pdxmesh = "planet_no_pole_no_clouds_mesh"
	
	scale = 0.98

	# Rings
	locator = { name = "root"  rotation = { 0 0 0 } }
	locator = { name = "root2" rotation = { 0 0 0 } }
